{
  "name": "v0-cli",
  "version": "1.1.5",
  "description": "CLI tool for v0 SDK",
  "main": "dist/index.js",
  "bin": {
    "v0": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage.enabled true --coverage.provider v8 --reporter=basic",
    "prune:dist": "node -e \"require('fs').rmSync('dist/__tests__', { recursive: true, force: true })\"",
    "prepublishOnly": "pnpm run build && pnpm run prune:dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "v0-sdk": "^0.7.1",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "v0",
    "cli",
    "vercel",
    "ai",
    "chat"
  ],
  "author": "Emmanuel Martinez",
  "repository": {
    "type": "git",
    "url": "https://github.com/emmanuel-martinez-dev/v0-cli.git",
    "directory": "cli"
  },
  "homepage": "https://github.com/emmanuel-martinez-dev/v0-cli#readme",
  "bugs": {
    "url": "https://github.com/emmanuel-martinez-dev/v0-cli#readme"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
